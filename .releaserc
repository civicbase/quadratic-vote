branches:
  - main
  - name: alpha
    prerelease: true
debug: true
ci: true
dryRun: false
plugins:
  - '@semantic-release/commit-analyzer'
  - '@semantic-release/release-notes-generator'
  # GitHub Releases + git tags are created by semantic-release.
  # npm publishing is handled separately by `.github/workflows/npm-publish.yml`
  # via npm Trusted Publishing (OIDC) on tag pushes.
  - - '@semantic-release/github'
    - successComment: false
      failComment: false
